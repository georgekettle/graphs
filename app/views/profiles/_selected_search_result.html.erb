<div class="selected-search-result" id="profile-<%= profile.id %>" data-profile="<%= profile.to_json %>" data-action='click->user-select#deselectResult'>
  <%= render('components/avatars/avatar', profile: profile, size: "large") %>
  <h4><%= profile.first_name.truncate(10) %></h4>
  <h4><%= profile.last_name.truncate(10) %></h4>
  <div class="remove">
    <%= render 'icons/close' %>
  </div>
</div>
